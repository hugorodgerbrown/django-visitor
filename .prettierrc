{
    "printWidth": 100,
    "tabWidth": 4,
    "useTabs": false,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "none",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "proseWrap": "always",
    "endOfLine": "auto",
    "overrides": [
        {
            "files": "*.yaml,*.yml",
            "options": { "tabWidth": 2 }
        }
    ]
}
